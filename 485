class Solution {
    public int findMaxConsectiiveOnes(int[] nums){
        int freq=0;max=0;
        for(int i=0;i<nums.lenght;i++){
            int e1.nums[i];
            if(e1==1){
                freq++;
                if(freq>max){
                    max=freq;
                
                }
            }
        if(e1==0){
            freq=0;
        }
    }
    }
}
